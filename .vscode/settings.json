{
  "liveSassCompile.settings.formats": [
    // You can add more
    {
      "format": "expanded",
      "extensionName": ".css",
      "savePath": "/css"
    },
    // More Complex
    {
      "format": "compressed",
      "extensionName": ".min.css",
      "savePath": "/css"
    }
  ],
  "liveSassCompile.settings.autoprefix": [
    "> 1%",
    "last 2 versions"
  ],
  "compile-hero.disable-compile-files-on-did-save-code": true
}